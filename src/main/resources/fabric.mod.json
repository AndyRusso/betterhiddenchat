	],
	"contact": {
		"homepage": "https://modrinth.com/mod/betterhiddenchat",
		"sources": "https://github.com/andyrusso/betterhiddenchat",
		"issues": "https://github.com/andyrusso/betterhiddenchat/issues"
	},
	"license": "MIT",
	"icon": "assets/betterhiddenchat/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"io.github.andyrusso.betterhiddenchat.BetterHiddenChat"
