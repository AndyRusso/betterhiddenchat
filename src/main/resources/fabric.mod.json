	"mixins": [
		"betterhiddenchat.mixins.json"
	],
	"accessWidener": "betterhiddenchat.accesswidener",
	"depends": {
		"fabricloader": ">=0.14",
		"minecraft": ">=1.19",
		"java": ">=17",
